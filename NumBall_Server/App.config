<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="addr" value="net.tcp://192.168.1.30:6000/Game"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="FileServiceBehavior" name="ConsoleApp1.MainService">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration="DuplexBinding"
          contract="ConsoleApp1.IGameService" />
        <host>
          <baseAddresses>
            <add baseAddress="http://192.168.1.30:7000/GetService" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="DuplexBinding" maxBufferSize="2147483647" maxConnections="2" maxBufferPoolSize="524288"
        maxReceivedMessageSize="2147483647">
          <reliableSession enabled="true" />
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="FileServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>